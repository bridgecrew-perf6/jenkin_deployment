pipeline{
    agent any
    stages{
        stage("building the artifact"){
            steps{
                script{
                     echo "build the artifact"
                }
            }
        }
        stage("testing the stages"){
            steps{
                script{
                    echo "testing the artifact..."
                }
            }
        }
        stage("deploying tthe artifact"){
            steps{
                script{
                     echo "deployed successfully the first jenkin build"
                }
            }
        }
    }
}